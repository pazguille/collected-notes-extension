{
  "version": "1.1.0",
  "name": "Collected Notes",
  "description": "Create new notes on Collected Notes",
  "manifest_version": 2,
  "icons": {
    "16": "assets/icon.png"
  },
  "permissions": ["contextMenus", "storage", "tabs", "https://collectednotes.com/*"],
  "background": {
    "persistent": false,
    "scripts": ["src/background.js"]
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_title": "Collected Notes"
  },
  "options_page": "src/options.html"
}
